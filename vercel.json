{
  "crons": [
    {
      "path": "/api/keep-alive",
      "schedule": "0 12 * * *"
    }
  ],
  "functions": {
    "src/pages/api/keep-alive.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/keep-alive",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}